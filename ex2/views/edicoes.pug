extends layout

block content
  .w3-card-4
    header.w3-container.w3-green
      h1= titulo
    .w3-container 
      table.w3-table-all 
        tr 
          td Id:
          td Ano:
          td Organizador:
          td Vencedor:
        each edicao in lEdicoes
          tr 
            td
             a(href='/' + edicao._id)= edicao._id
            td= edicao.anoEdição
            td
             a(href='/paises/:' + edicao.organizacao)= edicao.organizacao
            td
             a(href='/paises/:' + edicao.vencedor)= edicao.vencedor
    footer.w3-container.w3-green
      address Generated by ENGWEB2025